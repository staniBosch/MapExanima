<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<appSettings>

		<!-- Debug-Mode for calibration and other information of the game-->
		<add key="debug_" value="true"/>
		
		<!-- static Adresses from the memory Examinia Locations 
		             !DONT CHANGE THEM!
		-->
		
		<add key="OFFSET_X_Ptr" value="0x2A7FF4"/>
